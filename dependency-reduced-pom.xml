      <id>docker-windows</id>
      <build>
        <plugins>
          <plugin>
            <groupId>io.fabric8</groupId>
            <artifactId>docker-maven-plugin</artifactId>
            <version>${docker.maven.plugin.version}</version>
            <configuration>
              <images>
                <image>
                  <alias>ws-simple</alias>
                  <name>artifacts.msap.io/mulesoft-onprem/windows-soapui-for-wsc</name>
                </image>
                <image>
                  <alias>ws-simple-soap12</alias>
                  <name>artifacts.msap.io/mulesoft-onprem/windows-soapui-for-wsc</name>
                </image>
              </images>
            </configuration>
          </plugin>
        </plugins>
      </build>
      <properties>
        <docker.soapui.container.project.home>C:\Users\Administrator\${docker.soapui.container.project.folder}\</docker.soapui.container.project.home>
        <docker.soapui.container.user.home.url>file:///C:/Users/Administrator</docker.soapui.container.user.home.url>
        <docker.soapui.container.user.home.path>C:/Users/Administrator</docker.soapui.container.user.home.path>
